class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {

        int ele1, ele2;
        int count1 = 0, count2 = 0;

        for (int i = 0; i < nums.size(); i++) {

            if (count1 == 0 && nums[i] != ele2) {
                count1++;
                ele1 = nums[i];

            } else if (count2 == 0 && nums[i] != ele1) {
                count2++;
                ele2 = nums[i];

            } else if (ele1 == nums[i]) {
                count1++;
            } else if (ele2 == nums[i]) {
                count2++;
            } else {
                count1--, count2--;
            }
        }
        vector<int> ls; // list of answer

        // verification
        int cnt1 = 0, cnt2 = 0;
        for (int i = 0; i < nums.size(); i++) {

            if (ele1 == nums[i]) {
                cnt1++;
            }

            if (ele2 == nums[i])
                cnt2++;
        }

        if (cnt1 > (nums.size() / 3))
            ls.push_back(ele1);
        if (cnt2 > (nums.size() / 3))
            ls.push_back(ele2);

        return ls;
    }
};
