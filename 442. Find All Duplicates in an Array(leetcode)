class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int>tempNums(nums.size(),0);
        vector<int>ans;
        for(int i =0; i<nums.size();i++){
            tempNums[nums[i]-1]++;
        }

        for(int i =0;i<tempNums.size();i++){
             if(tempNums[i]==2){
                ans.push_back(i+1);
             }
        }
        return ans;
    }
};
