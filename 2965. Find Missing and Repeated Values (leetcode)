class Solution {
public:
    vector<int> findMissingAndRepeatedValues(vector<vector<int>>& grid) {
        // i am using the map data structure
        vector<int> ans;
        unordered_map<int, int> frequencyOfElement;
        int n = grid.size();
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                frequencyOfElement[grid[i][j]]++;
            }
        }

        for (int i = 1; i <= n * n; i++) {
            if (frequencyOfElement[i] > 1) {
                ans.push_back(i);
            }

        }

        for (int i = 1; i <= n * n; i++) {

            if (frequencyOfElement[i] == 0) {
                ans.push_back(i);
            }
        }

        return ans;
    }
};
