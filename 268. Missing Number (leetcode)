class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int firstNums = nums[0];
        for (int i = 1; i < nums.size(); i++) {
            firstNums = firstNums ^ nums[i];
        }
        for (int i = 0; i <= nums.size(); i++) {
            firstNums = firstNums ^ i;
        }

        return firstNums;
    }
};
